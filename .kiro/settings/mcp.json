{
  "mcpServers": {
    "chrome-devtools": {
      "command": "npx",
      "args": [
        "-y",
        "chrome-devtools-mcp"
      ],
      "disabled": false,
      "autoApprove": [
        "new_page",
        "fill",
        "evaluate_script",
        "press_key",
        "take_screenshot",
        "list_console_messages",
        "resize_page",
        "navigate_page",
        "performance_start_trace",
        "performance_analyze_insight"
      ]
    },
    "figma": {
      "url": "http://127.0.0.1:3845/mcp",
      "disabled": false,
      "autoApprove": [
        "get_metadata"
      ]
    }
  }
}